{
  "name": "Tipalti Titles",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This gives proper page titles in Tipalti AP Hub.",
  "homepage_url": "https://github.com/nquinlan/tipalti-titles",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://aphub2.tipalti.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://aphub2.tipalti.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}